    <script type="text/javascript">
      function selectCat(){
        $("#rate").hide("slow");

        if(document.getElementById("form_category").value=="Saran"){
          $("#rate").hide("slow");
          document.getElementById("form_status").value = 0;
          document.getElementById("form_show_only").value = 0;
        }else if(document.getElementById("form_category").value=="Kritik"){
          $("#rate").hide("slow");
          document.getElementById("form_status").value = 0;
          document.getElementById("form_show_only").value = 0;
        }else if(document.getElementById("form_category").value=="Pertanyaan"){
          $("#rate").hide("slow");
          document.getElementById("form_status").value = 0;
          document.getElementById("form_show_only").value = 0;
        }else if(document.getElementById("form_category").value=="Testimonial"){
          document.getElementById("form_status").value = 1;
          document.getElementById("form_show_only").value = 1;
          $("#rate").show("slow");
        }else{
          $("#rate").hide("slow");
          document.getElementById("form_status").value = 0;
          document.getElementById("form_show_only").value = 0;
        }
      }
      
    </script>
