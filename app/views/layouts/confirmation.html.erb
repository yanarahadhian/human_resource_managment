    <script type="text/javascript">
      $(document).ready(function(){

        $('#account_menu').hide()
        $('#account_menu_trigger').click(function(){
          $('#account_menu').toggle();
          $(this).toggleClass('account-menu-trigger');
          $(this).toggleClass('account-menu-trigger-disable');
        })
        $('#account_menu')
        .bind('mouseover',function(){
          $(this).show();
        });

        $('#mainContent').click(function(){
          $('#account_menu').hide();
          $('#account_menu_trigger').removeClass('account-menu-trigger');
          $('#account_menu_trigger').addClass('account-menu-trigger-disable');
        });

        $("a[rel='example3']").colorbox({transition:"none", width:"50%", height:"75%"});
        $(".testimoni1").hide();
        $('#aktiv').click(function(){
          $('#titik').hide();
          $(".testimoni1").show('300');
          $('#aktiv').hide();
          return false;
        });

        $('#full').click(function(){
          $(".testimoni1").hide('400');
          $('#titik').show('300');
          $('#aktiv').show('300');
          return false;
        });
        //==============================================================================
        $(".feedback").colorbox({width:"auto",height:"auto", inline:true, href:"#feedback-area"});
        $(".login").colorbox({width:"auto",height:"auto", inline:true, href:"#Sign_In"});
        $("#loginfb").colorbox({width:"auto",height:"auto", inline:true, href:"#login_fb"});
        //datepicker
        $(".datepicker").datepicker();
        //filter selected
        $('.active').children("input:radio").attr('checked',"checked");
        $(".field").click(function(){
          $(this).children("input:radio").attr('checked',"checked");
          $(".field").removeClass("active");
          $(".field").each(function(){
            if($(this).children("input:radio").attr('checked')){
              $(this).addClass("active");
            }
          });
        });
      });
      $(function(){ // wait for document to load
        $('#filter_user_rating input.star').rating();

        $("a.switch_desc").toggle(function(){
          $(this).addClass("swap");
          $("ul.product").fadeOut("fast", function() {
            $(this).fadeIn("fast").addClass("product_desc");
          });
        }, function () {
          $(this).removeClass("swap");
          $("ul.product").fadeOut("fast", function() {
            $(this).fadeIn("fast").removeClass("product_desc");
          });
        });
      });
    </script>

    <script type="text/javascript">
      function show_filter_by(thisObj){
        if($('#filter_user_rating').css('display')!='none') $('#filter_user_rating').hide();
        if($('#filter_users').css('display')!='none') $('#filter_users').hide();
        if($('#filter_price').css('display')!='none') $('#filter_price').hide();
        if($('#filter_comments').css('display')!='none') $('#filter_comments').hide();
        if($('#filter_release_date').css('display')!='none') $('#filter_release_date').hide();
        if($('#filter_industry').css('display')!='none') $('#filter_industry').hide();

        if(thisObj.value=='user rating'){
          $('#filter_user_rating').slideToggle("normal", "linear");
        }else
          if(thisObj.value=='users'){
            $('#filter_users').slideToggle("normal", "linear");
          }else
          if(thisObj.value=='price'){
            $('#filter_price').slideToggle("normal", "linear");
          }else
          if(thisObj.value=='comments'){
            $('#filter_comments').slideToggle("normal", "linear");
          }else
          if(thisObj.value=='release date'){
            $('#filter_release_date').slideToggle("normal", "linear");
          }else
          if(thisObj.value=='industry'){
            $('#filter_industry').slideToggle("normal", "linear");
          }
      }
    </script>

    <script type="text/javascript">
