<!-- popup -->
<div style="display:none;">
  <div class="frm-popup" id="popup_export_report_lembur">
    <div id="popup2">
      <div class="title-page-tambah">Export Report</div>
      <fieldset>
        <form id="super_form" method="get" action="/overtimes/overtime_report_export">
          <span class="input">
            <label>Tanggal Mulai</label> :
            <%= text_field_tag "popup_lembur_temp_date",{},{:style => "width:140px !important;"}%>
            <%= hidden_field_tag :date_start,{},{:id => "popup_lembur_date_pick"} %>
          </span>
          <div class="clear"></div>
          <span class="input">
            <label>Tanggal Selesai</label> :
            <%= text_field_tag "popup_lembur_temp_date2",{},{:style => "width:140px !important;"}%>
            <%= hidden_field_tag :date_end,{},{:id => "popup_lembur_date_pick2"} %>
          </span>
          <div class="clear"></div>
          <hr>
          <div class="center-text  box-form">
            <input type="reset" id="info_section_cancel_basic" class="grey_button" value="Batal" name="info_section_cancel_basic" onclick="javascript:$.colorbox.close();">
            <input type="submit" value="Simpan" name="info_section_save_basic" id="info_section_save_basic" class="blue_button" onclick="javascript:$.colorbox.close();">
          </div>
        </form>
      </fieldset>
    </div>
  </div>
</div>
<!-- end popup -->
<script type="text/javascript">
  $(document).ready(function(){

  });
</script>

